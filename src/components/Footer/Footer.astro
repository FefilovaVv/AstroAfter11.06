---
import Footer_bottomPart from "./Footer_bottomPart.astro";
import Footer_upperPart from "./Footer_upperPart.astro";
---

<style>
    footer {
        padding: 50px 60px;
        background-color: rgb(247, 247, 247);
    }
    @media (min-width: 0px) and (max-width: 1100px) {
        footer {
            padding: 50px 40px;
        }
    }
    @media (min-width: 0px) and (max-width: 750px) {
        footer {
            padding: 0px 20px;
        }
    }
</style>
<footer>
    <Footer_upperPart />
    <Footer_bottomPart />
</footer>
